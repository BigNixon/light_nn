#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x564242a115c0 .scope module, "tb_ROM" "tb_ROM" 2 4;
 .timescale -9 -12;
P_0x5642429fbaa0 .param/l "ADDR_WIDTH" 1 2 7, +C4<00000000000000000000000000001100>;
P_0x5642429fbae0 .param/l "DATA_WIDTH" 1 2 8, +C4<00000000000000000000000000001000>;
v0x564242a23f00_0 .var "addr", 11 0;
v0x564242a23ff0_0 .var "clk", 0 0;
v0x564242a240c0_0 .net "data", 7 0, v0x564242a23cd0_0;  1 drivers
S_0x564242a11820 .scope module, "uut" "rom" 2 16, 3 1 0, S_0x564242a115c0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 12 "addr";
    .port_info 2 /OUTPUT 8 "data";
P_0x5642429fcbb0 .param/l "ADDR_WIDTH" 0 3 1, +C4<00000000000000000000000000001100>;
P_0x5642429fcbf0 .param/l "DATA_WIDTH" 0 3 1, +C4<00000000000000000000000000001000>;
v0x5642429d6cf0_0 .net "addr", 11 0, v0x564242a23f00_0;  1 drivers
v0x564242a23c10_0 .net "clk", 0 0, v0x564242a23ff0_0;  1 drivers
v0x564242a23cd0_0 .var "data", 7 0;
v0x564242a23dc0 .array "mem", 0 4095, 7 0;
E_0x564242a0fbe0 .event posedge, v0x564242a23c10_0;
    .scope S_0x564242a11820;
T_0 ;
    %vpi_call 3 12 "$readmemb", "sigmoid_12_bit.txt", v0x564242a23dc0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x564242a11820;
T_1 ;
    %wait E_0x564242a0fbe0;
    %load/vec4 v0x5642429d6cf0_0;
    %pad/u 14;
    %ix/vec4 4;
    %load/vec4a v0x564242a23dc0, 4;
    %assign/vec4 v0x564242a23cd0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x564242a115c0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x564242a23ff0_0;
    %inv;
    %store/vec4 v0x564242a23ff0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x564242a115c0;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564242a23ff0_0, 0, 1;
    %pushi/vec4 0, 0, 12;
    %store/vec4 v0x564242a23f00_0, 0, 12;
    %vpi_call 2 34 "$dumpfile", "tb_ROM.vcd" {0 0 0};
    %vpi_call 2 37 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564242a115c0 {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 10, 0, 12;
    %store/vec4 v0x564242a23f00_0, 0, 12;
    %delay 20000, 0;
    %vpi_call 2 49 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "rom_tb.sv";
    "./rom.sv";
